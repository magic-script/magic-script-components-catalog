<manifest xmlns:ml="magicleap"
          ml:package="org.magicscript.catalog"
          ml:version_name="0.0.1"
          ml:version_code="1">
  <application ml:visible_name="Catalog"
               ml:sdk_version="0.24.0"
               ml:min_api_level="8">
    <component ml:name=".universe"
               ml:visible_name="Catalog"
               ml:binary_name="bin/index.js"
               ml:type="Universe">
      <mode ml:shareable="true"/>
      <icon ml:model_folder="res/Icon/model"
            ml:portal_folder="res/Icon/portal"/>
      <schema ml:name="catalog://" />
    </component>
    <uses-privilege ml:name="MagicScript"/>
    <uses-privilege ml:name="Internet"/>
    <uses-privilege ml:name="WebView"/>
  </application>
</manifest>
